:key=\"item.name\"\n      v-on:click=\"onItemClick(index)\"\n    >\n      <label class=\"item-name\">{{item.name}}</label>\n      <label class=\"item-price\">{{item.salePrice}}元</label>\n    </li>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport store from \"@/store.js\";\n// var store = require('@/store');\n\nexport default {\n  name: \"home\",\n  data() {\n    return {\n      keyword: null,\n      goodsItems: null,\n      filteredItems: null\n    };\n  },\n  created() {\n    this.requestGoods();\n  },\n  mounted() {},\n  methods: {\n    requestGoods() {\n      axios\n        .post(\"/api_getAllGoods\")\n        .then(response => {\n          // eslint-disable-next-line\n          console.log(response.data);\n          this.goodsItems = response.data.filter(it => it.count > 0).reverse();\n        })\n        .catch(error => {\n          // eslint-disable-next-line\n          console.log(error);\n        });\n    },\n    onItemClick(index) {\n      this.$toast(\"已添加到购物车\");\n      //添加到全局 store 中\n      store.store.addItem2Cart(this.goodsItems[index]);\n      // eslint-disable-next-line\n      console.log(store);\n    },\n    onSearch() {\n      this.filteredItems = this.goodsItems.filter(it =>\n        it.name.includes(this.keyword)\n      );\n    }\n  }\n};\n</script>\n\n<style>\n#home {\n  background-size: 100% 100%;\n  width: 100%;\n  height: 100%;\n}\n\n.search {\n  position: -webkit-sticky; /* Safari */\n  position: sticky;\n  top: 0;\n}\n.goods-item {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  width: 80%;\n  padding-left: 10%;\n  padding-right: 10%;\n  height: 5vh;\n  -webkit-transition-duration: 0.2s; /* Safari */\n  transition-duration: 0.2s;\n}\n\n.goods-item:hover {\n  background-color: #e7e7e7;\n}\n\n.item-name {\n  /* text-align: start; */\n  margin-right: 5%;\n}\n.item-price {\n  white-space: nowrap;\n}\n</style>\n\n"],"sourceRoot":"src/pages"}]}