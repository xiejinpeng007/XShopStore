  var params = new URLSearchParams();\n      params.append(\"token\", cookieutil.getCookie(\"token\"));\n      params.append(\"start\", \"\");\n      params.append(\"end\", \"\");\n      params.append(\"offset\", 0);\n      params.append(\"limit\", 0);\n      axios.post(\"/api_getChargeHistory\", params).then(function (response) {\n        _this.historyItems = response.data.chargeHistory;\n        _this.sumCharge = response.data.chargeHistory.map(function (it) {\n          return it.charge;\n        }).reduce(function (pre, cur) {\n          return pre + cur;\n        });\n        console.log(response.data);\n      }).catch(function (error) {\n        console.log(error);\n\n        _this.$toast(\"请求失败，请检查网络\");\n      });\n    },\n    timestamp2String: function timestamp2String(time) {\n      return TimeUtil.TimeUtil.timestamp2String(time);\n    }\n  }\n};",{"version":3,"sources":["chargehistory.vue"],"names":[],"mappings":";;;;;;;;;;;;;;AAeA,OAAA,KAAA,MAAA,OAAA;AACA,OAAA,UAAA,MAAA,uBAAA;AACA,OAAA,QAAA,MAAA,qBAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,eADA;AAEA,EAAA,IAFA,kBAEA;AACA,WAAA;AACA,MAAA,YAAA,EAAA,EADA;AAEA,MAAA,SAAA,EAAA;AAFA,KAAA;AAIA,GAPA;AAQA,EAAA,OARA,qBAQA;AACA,SAAA,oBAAA;AACA,GAVA;AAWA,EAAA,OAAA,EAAA;AACA,IAAA,oBADA,kCACA;AAAA;;AACA,UAAA,MAAA,GAAA,IAAA,eAAA,EAAA;AACA,MAAA,MAAA,CAAA,MAAA,CAAA,OAAA,EAAA,UAAA,CAAA,SAAA,CAAA,OAAA,CAAA;AACA,MAAA,MAAA,CAAA,MAAA,CAAA,OAAA,EAAA,EAAA;AACA,MAAA,MAAA,CAAA,MAAA,CAAA,KAAA,EAAA,EAAA;AACA,MAAA,MAAA,CAAA,MAAA,CAAA,QAAA,EAAA,CAAA;AACA,MAAA,MAAA,CAAA,MAAA,CAAA,OAAA,EAAA,CAAA;AACA,MAAA,KAAA,CACA,IADA,CACA,uBADA,EACA,MADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,YAAA,GAAA,QAAA,CAAA,IAAA,CAAA,aAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,QAAA,CAAA,IAAA,CAAA,aAAA,CACA,GADA,CACA,UAAA,EAAA;AAAA,iBAAA,EAAA,CAAA,MAAA;AAAA,SADA,EAEA,MAFA,CAEA,UAAA,GAAA,EAAA,GAAA,EAAA;AACA,iBAAA,GAAA,GAAA,GAAA;AACA,SAJA,CAAA;AAKA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,OAVA,EAWA,KAXA,CAWA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;;AACA,QAAA,K