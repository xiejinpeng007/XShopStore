  </div>\n    <div v-else>\n      <button class=\"login-button bg-gradual-orange\" v-on:click=\"onBackCart\">返回购物车</button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport store from \"@/store.js\";\nimport cookieutil from \"@/utils/cookieutil.js\";\n\nexport default {\n  name: \"order\",\n  data() {\n    return {\n      goodsOrder: store.store.orderGoodsItems,\n      totalPrice: store.store.getTotalPrice(),\n      orderPaied: false\n    };\n  },\n  created() {},\n  methods: {\n    onPay() {\n      const cart = this.goodsOrder\n        .map(it => it.barCode + \",\" + it.quantity)\n        .join(\";\")\n        .toString();\n      const params = new URLSearchParams();\n      params.append(\"cart\", cart);\n      params.append(\"token\", cookieutil.getCookie(\"token\"));\n      axios\n        .post(\"/api_payManyGoods\", params)\n        .then(response => {\n          console.log(response.data);\n          if (response.data.status == \"OK\") {\n            store.store.clearOrderAndRemoveFromCart();\n            this.orderPaied = true;\n          } else {\n            this.$toast(\"支付失败，请检查网络\");\n          }\n        })\n        .catch(error => console.log(error));\n    },\n    onBackCart() {}\n  }\n};\n</script>\n\n<style>\n#order {\n  background-color: #f8f8f8;\n  overflow: hidden;\n  height: 100%;\n}\n.order-item {\n  align-items: center;\n  align-content: center;\n  padding-right: 5%;\n  padding-left: 5%;\n  background-color: white;\n  height: 5vh;\n  display: flex;\n  justify-content: space-between;\n}\n</style>\n\n"],"sourceRoot":"src/pages"}]}